#!/usr/bin/python
import boto3
ec2=boto3.resource('ec2')
for i in ec2.volumes.page_size(30):
    if i.state=='in-use':
       if i.tags==None:
          id=i.attachments[0]['InstanceId']
          instance = ec2.Instance(id)
          if instance.tags[0]['Key']=='Name':
             name=instance.tags[0]['Value']
             print i.id,id,name

